    "dayjs": "^1.10.6",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^8.5.1",
    "mime": "^3.0.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.7.0",
    "pg": "^8.6.0",
    "rate-limiter-flexible": "^2.3.4",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
